{
	"name": "ADXLoadExample",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": "SampleData",
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureDataExplorerSink",
						"ingestionMappingName": "Map1"
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"translator": {
						"type": "TabularTranslator",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "JsonHedSalesDS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "HEDSalesAZXDS",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}