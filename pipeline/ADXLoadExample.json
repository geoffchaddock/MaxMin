{
	"name": "ADXLoadExample",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Azure Data Explorer Command1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": "SampleData",
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureDataExplorerSink",
						"ingestionMappingName": ""
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"translator": {
						"type": "TabularTranslator",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "JsonHedSalesDS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "HEDSalesAZXDS",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Azure Data Explorer Command1",
				"type": "AzureDataExplorerCommand",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": ".clear table tbkHedSalesUK data ",
					"commandTimeout": "00:20:00"
				},
				"linkedServiceName": {
					"referenceName": "HEDSalesAZXLS",
					"type": "LinkedServiceReference"
				}
			}
		],
		"annotations": []
	}
}